{
  "name": "jest-pixel-perfect",
  "version": "0.1.1",
  "description": "Jest matcher to check if your frontend matches the design",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run clean && tsc -p ./tsconfig.json",
    "watch": "npm run build -- --watch",
    "clean": "rm -rf lib",
    "prepublishOnly": "npm run build",
    "test": "source .env && jest --config jest.config.js --no-watchman"
  },
  "keywords": [
    "jest",
    "matcher",
    "pixel perfect",
    "design",
    "frontend"
  ],
  "files": [
    "lib"
  ],
  "author": "Mathieu Dutour <mathieu@dutour.me>",
  "repository": {
    "url": "https://github.com/mathieudutour/jest-pixel-perfect"
  },
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^3.1.1",
    "pixelmatch": "^5.2.0",
    "pngjs": "^5.0.0"
  },
  "peerDependencies": {
    "jest": "*"
  },
  "devDependencies": {
    "@types/jest": "^25.2.2",
    "@types/node": "^14.0.1",
    "@types/node-fetch": "^2.5.7",
    "@types/pixelmatch": "^5.2.1",
    "@types/pngjs": "^3.4.2",
    "jest": "^26.0.1",
    "jest-cli": "^26.0.1",
    "jest-matcher-utils": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.0.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2"
  }
}
